[
  {
    "id": "mock-001",
    "enabled": true,
    "match": {
      "method": "GET",
      "path": "/users/:id"
    },
    "response": {
      "status": 200,
      "delay": 1000,
      "body": {
        "id": "{{ uuid() }}",
        "teste": "{{ query.ua || 'nao tem' }}",
        "name": "{{ faker.person.fullName() }}",
        "teste_sdasdjkajsd": "{{ name }}",
        "email": "{{ faker.internet.email() }}",
        "avatar": "{{ faker.image.avatar() }}",
        "address": {
          "street": "{{ faker.location.street() }}",
          "city": "{{ faker.location.city() }}"
        },
        "items": "{{ Array.from({length: randomInt(2,5)}, () => ({ id: uuid(), name: faker.commerce.productName() })) }}"
      }
    }
  },
  {
    "id": "mock-001",
    "enabled": true,
    "triggerError": false,
    "match": {
      "method": "GET",
      "path": "/bfffeatureflagapi/featureflag/getall"
    },
    "error": {
      "status": 401,
      "body": {
        "message": "mensagem {{ uuid() }} {{ body.email }} de error body"
      }
    },
    "response": {
      "status": 200,
      "delay": 1000,
      "body": {
        "id": "{{ uuid() }}",
        "teste": "{{ query.ua || 'nao tem' }}",
        "name": "{{ faker.person.fullName() }}",
        "teste_sdasdjkajsd": "{{ name }}",
        "email": "{{ faker.internet.email() }}",
        "avatar": "{{ faker.image.avatar() }}",
        "address": {
          "street": "{{ faker.location.street() }}",
          "city": "{{ faker.location.city() }}"
        },
        "items": "{{ Array.from({length: randomInt(2,5)}, (_, i) => ({ id: uuid(), name: faker.commerce.productName(), isFirst: i === 0 })) }}"
      }
    }
  }
]
